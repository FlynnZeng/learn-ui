    "@learn-ui-to-me/vite-plugins": "workspace:*",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitepress-preview/component": "workspace:*",
    "learn-ui-to-me": "workspace:*",
    "lodash-es": "^4.17.21",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
