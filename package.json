    "@types/shelljs": "^0.8.16",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "async-validator": "^4.2.5",
    "cross-env": "^7.0.3",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "release-it": "^19.0.2",
    "shelljs": "^0.10.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vite-plugin-dts": "3.9.1",
    "vitest": "^3.1.2",
    "vue3-i18n": "^1.1.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@learn-ui-to-me/components": "workspace:*",
    "@learn-ui-to-me/hooks": "workspace:*",
    "@learn-ui-to-me/theme": "workspace:*",
    "@learn-ui-to-me/utils": "workspace:*",
