{
  "name": "learn-ui-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --filter @learn-ui-to-me/playground dev",
    "docs:dev": "pnpm --filter @learn-ui-to-me/docs dev",
    "docs:build": "pnpm --filter @learn-ui-to-me/docs build",
    "storybook": "pnpm --filter @learn-ui-to-me/playground storybook",
    "test": "run-p test:utils test:comp",
    "test:hooks": "cross-env NODE_ENV=test pnpm --filter @learn-ui-to-me/hooks test",
    "test:comp": "cross-env NODE_ENV=test pnpm --filter @learn-ui-to-me/components test",
    "test:utils": "cross-env NODE_ENV=test pnpm --filter @learn-ui-to-me/utils test",
    "build": "run-s build-hooks build-components",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @learn-ui-to-me/hooks build",
    "build-components": "cross-env NODE_ENV=production pnpm --filter learn-ui-to-me build",
    "build:dev": "run-s build-hooks:dev build-components:dev",
    "build-hooks:dev": "cross-env NODE_ENV=development pnpm --filter @learn-ui-to-me/hooks build",
    "build-components:dev": "cross-env NODE_ENV=development pnpm --filter learn-ui-to-me build:watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
